{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "HTTP Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "fillOpacity": 20 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count{job=\"gridappsd\"}[5m])) by (http_route)",
          "legendFormat": "{{http_route}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "HTTP Request Duration (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "fillOpacity": 10 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_request_duration_seconds_bucket{job=\"gridappsd\"}[5m])) by (le, http_route)) * 1000",
          "legendFormat": "{{http_route}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "HTTP Client Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "fillOpacity": 20 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(http_client_request_duration_seconds_count{job=\"gridappsd\"}[5m])) by (http_request_method)",
          "legendFormat": "{{http_request_method}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "HTTP Client Duration (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "fillOpacity": 10 }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_client_request_duration_seconds_bucket{job=\"gridappsd\"}[5m])) by (le)) * 1000",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Recent Traces",
      "type": "table",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "tempo", "uid": "${DS_TEMPO}" },
      "targets": [
        {
          "queryType": "traceqlSearch",
          "filters": [
            { "id": "service-name", "tag": "service.name", "operator": "=", "value": ["gridappsd"], "scope": "resource" }
          ],
          "limit": 20,
          "refId": "A"
        }
      ]
    },
    {
      "title": "Service Map",
      "type": "nodeGraph",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 28 },
      "datasource": { "type": "tempo", "uid": "${DS_TEMPO}" },
      "targets": [
        {
          "queryType": "serviceMap",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["gridappsd", "traces"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "name": "DS_PROMETHEUS",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "current": { "selected": false, "text": "Tempo", "value": "Tempo" },
        "hide": 0,
        "includeAll": false,
        "name": "DS_TEMPO",
        "options": [],
        "query": "tempo",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "GridAPPS-D Traces",
  "uid": "gridappsd-traces"
}
