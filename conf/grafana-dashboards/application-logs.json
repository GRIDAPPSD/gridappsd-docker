{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Log Volume by Level",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 40,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "ERROR" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "WARN" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "INFO" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "DEBUG" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      },
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({job=\"gridappsd\"} |= `$simulation_id` [$__auto]))",
          "legendFormat": "{{level}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Logs",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 6 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      },
      "targets": [
        {
          "expr": "{job=\"gridappsd\", level=~\"ERROR|WARN\"} |= `$simulation_id`",
          "refId": "A"
        }
      ]
    },
    {
      "title": "All Logs",
      "type": "logs",
      "gridPos": { "h": 14, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      },
      "targets": [
        {
          "expr": "{job=\"gridappsd\"} |= `$simulation_id` |= `$search`",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["gridappsd", "logs"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Loki", "value": "Loki" },
        "hide": 0,
        "includeAll": false,
        "name": "DS_LOKI",
        "options": [],
        "query": "loki",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "current": { "selected": false, "text": "", "value": "" },
        "hide": 0,
        "name": "simulation_id",
        "options": [],
        "query": "",
        "type": "textbox",
        "label": "Simulation ID"
      },
      {
        "current": { "selected": false, "text": "", "value": "" },
        "hide": 0,
        "name": "search",
        "options": [],
        "query": "",
        "type": "textbox",
        "label": "Search"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "GridAPPS-D Logs",
  "uid": "gridappsd-logs"
}
